l(ageInterval);\r\n                }else{\r\n                    setAge(a => a + 1 );\r\n                }                                \r\n            },1000)\r\n        }\r\n        return () => clearInterval(ageInterval);\r\n    }, [done]);  \r\n    return(\r\n    <li\r\n        className={classNames(\r\n            'wish-list__item',{\r\n            'wish-list__item--done': done,\r\n            'wish-list__item--warning': age >= 5 && age < 10,\r\n            'wish-list__item--danger': age >= 10,\r\n            }\r\n        )}    \r\n    >\r\n        <input type=\"checkbox\" id={id} checked={done} onChange={e => onDoneChange(e.target.checked)}/>            \r\n        <label htmlFor={id}>{text}</label>\r\n    </li>\r\n)};\r\n\r\nWishItem.propTypes = {\r\n    done: PropTypes.bool,\r\n    text: PropTypes.string,\r\n    id: PropTypes.string,\r\n    onDoneChange: PropTypes.func\r\n};\r\n\r\nWishItem.defaultProps = {\r\n    done: false,\r\n    text: '',\r\n    id: '',\r\n    onDoneChange: () =>{},\r\n}\r\n\r\nexport default WishItem;"},"lineCount":null}},"error":null,"hash":"2aab17f0314fc21e2dd4e1763603c3f2","cacheData":{"env":{}}}